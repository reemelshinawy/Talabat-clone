<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="Vbox" fillWidth="false" onMouseClicked="#click" prefHeight="290.0" prefWidth="200.0" style="-fx-background-color: #fff; -fx-background-radius: 10;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CustomerPack.ItemRestaurantController">
    <children>
        <ImageView fx:id="restaurantImg" fitHeight="202.0" fitWidth="199.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Res/kfc.png" />
            </image>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
        </ImageView>
        <Label fx:id="restaurantName" alignment="CENTER" prefHeight="0.0" prefWidth="200.0" text="KFC">
            <font>
                <Font name="Dubai Bold" size="26.0" />
            </font>
            <VBox.margin>
                <Insets top="15.0" />
            </VBox.margin>
        </Label>
    </children>
   <effect>
      <DropShadow />
   </effect>
</VBox>
